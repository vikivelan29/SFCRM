public class RNWL_AccountTriggerHandler extends ASF_VTriggerHandler{
    
    private Boolean isTriggerDisabled(){
        return isDisabled('Account');
    }

    public override void afterUpdate(Map<Id, SObject> newOpps, Map<Id, SObject> oldOpps) {
        
        if(isTriggerDisabled()){return;}
        RNWL_AccountTriggerHelper.updateOppsUponAccountDataChange((Map<Id, Account>)newOpps, (Map<Id, Account>)oldOpps);
    }

}