<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Claimant_Mobile_Number_Field_Validation</fullName>
    <active>true</active>
    <errorConditionFormula>AND(Claimant_Mobile_Number__c != &apos;&apos;,NOT(REGEX(Claimant_Mobile_Number__c, &quot;^[0-9]{2,15}$&quot;)))</errorConditionFormula>
    <errorDisplayField>Claimant_Mobile_Number__c</errorDisplayField>
    <errorMessage>Claimant Mobile Number should only have numbers.</errorMessage>
</ValidationRule>
