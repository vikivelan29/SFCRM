<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Amount_Less_Than_Zero</fullName>
    <active>true</active>
    <errorConditionFormula>OR(Refund_Amount__c &lt; 0, Diputed_Amount__c &lt; 0, Emi_Amount__c &lt; 0)</errorConditionFormula>
    <errorMessage>Amount can&apos;t be less than zero</errorMessage>
</ValidationRule>
