<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Cibil_Score_Validation</fullName>
    <active>true</active>
    <errorConditionFormula>OR(Current_cibil_score__c &lt; 300,Current_cibil_score__c &gt; 900)</errorConditionFormula>
