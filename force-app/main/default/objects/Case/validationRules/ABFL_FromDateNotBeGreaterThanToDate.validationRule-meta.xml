    <fullName>ABFL_FromDateNotBeGreaterThanToDate</fullName>
    <active>true</active>
    <description>Check from date &amp; to date</description>
    <errorConditionFormula>IsPickVal(Business_Unit__c,&apos;ABFL&apos;) &amp;&amp; 
