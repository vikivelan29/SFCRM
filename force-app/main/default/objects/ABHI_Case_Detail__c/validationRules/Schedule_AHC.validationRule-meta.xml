<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Schedule_AHC</fullName>
    <active>true</active>
    <errorConditionFormula>OR(Schedule_AHC_1st_Preference__c &gt; (Now()+7),Schedule_AHC_2nd_Preference__c &gt; (Now()+7))</errorConditionFormula>
    <errorMessage>Schedule AHC should be within 7 days of case creation.</errorMessage>
</ValidationRule>
