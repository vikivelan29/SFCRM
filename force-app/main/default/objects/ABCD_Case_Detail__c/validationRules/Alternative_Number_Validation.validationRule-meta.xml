<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Alternative_Number_Validation</fullName>
    <active>true</active>
    <errorConditionFormula>AND(
    NOT(ISBLANK(Alternative_Number__c)), 
    NOT(REGEX(Alternative_Number__c, &quot;^[0-9]{10}$&quot;))
)</errorConditionFormula>
    <errorDisplayField>Alternative_Number__c</errorDisplayField>
    <errorMessage>Alternative Number Should be 10 digit number.</errorMessage>
</ValidationRule>
